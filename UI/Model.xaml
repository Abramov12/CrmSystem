<Window
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:UI"
        xmlns:ui="http://schemas.lepo.co/wpfui/2022/xaml" x:Class="UI.Model"
        mc:Ignorable="d"
        Title="ComputerModel" Name="ModelForm" Height="245" Width="244" ResizeMode="NoResize" Background="#FF0F0F0F">
    <Grid >
        <Button Content="Create model" Click="ShowCashDesks" x:Name="BtnShow_Copy" Margin="0,135,0,0" VerticalAlignment="Top" Width="156" Height="37" HorizontalAlignment="Center"/>
        <Label Name="desiredQuantity" Content="Enter the number of desired cashdesks" HorizontalAlignment="Center" Margin="0,28,0,0" VerticalAlignment="Top"/>
        <ui:NumberBox Name="CashDesksCount" Value="3" HorizontalAlignment="Center" Margin="0,68,0,0" Maximum="10" Minimum="1" TextWrapping="Wrap" Text="1-10" VerticalAlignment="Top" Width="168"/>
    </Grid>
</Window>
